#include<stdio.h>
int input(int *a,int *b)
{ 
	 printf("enter the two number/n");
	scanf("%d",a);
	scanf("%d",b);
}
int compute(int a,int b)
{
	if(a>b)
{
	return b;
}
	if(b>a)
{
	return a;
}
}
int output(int x)
{
	printf("smallest number is %d",x);
}
void main()
{
	int a,b;
	input(&a,&b);
	
	int z=compute(a,b);
	output(z);
}	

         
